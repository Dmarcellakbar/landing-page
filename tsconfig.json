{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/content/FAQ/Banner.js", "components/Navbar/Navbar.js", "components/chat/Chat.js", "components/chat/crisp.js", "components/chat/CrispWithNoSSR.js", "components/chat/crisp.tjs"],
  "exclude": ["node_modules"]
}
